import java.util.*;

public class StsackWithLL{
    Node head;
    static class Node{
        String data;
        Node next;
        Node(String data){
            this.data = data;
            this.next = null;
        }
    }

    public static StsackWithLL push(StsackWithLL list, String data){
        Node new_node = new Node(data);
        if(list.head == null){
            list.head = new_node;
        }else{
           new_node.next = list.head;
           list.head = new_node;
        }
        return list;
    }

    public static StsackWithLL pop(StsackWithLL list){
        if(list.head == null){
            System.out.println("list is empty");
        }else{
            Node temp = list.head;
            list.head = temp.next;
            temp = null;
        }
        return list;
    }

    public void peek(StsackWithLL list){
        if(list.head == null){
            System.out.println("stack is empty ");
        }else{
            Node ptr = list.head;
            while(ptr != null){
                System.out.print(ptr.data+"->");
                ptr =ptr.next;
            }
            System.out.println();
        }
    }

    public static void main(String[] args) {
        StsackWithLL lsit = new StsackWithLL();
        
        push(lsit,"I");
        push(lsit,"V");
        push(lsit,"A");
        push(lsit,"R");
        peek(lsit);
        pop(lsit);
        peek(lsit);

    }
}